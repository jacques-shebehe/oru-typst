@article{Hilmer-2009,
  title = {The Assessment of Frailty in Older People in Acute Care},
  author = {Hilmer, Sarah N. and Perera, Vidya and Mitchell, Sarah and Murnion, Bridin P. and Dent, Jonathan and Bajorek, Beata and Matthews, Slade and Rolfson, Darryl B.},
