@article{Valencia-2014,
  title = {Pharmacological Treatment of Diabetes in Older People},
  author = {Valencia, W. M. and Florez, H.},
  year = {2014},
  month = dec,
  journal = {Diabetes Obes Metab},
  edition = {2014/07/31},
  volume = {16},
