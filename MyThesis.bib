@article{Kripalani-2006,
  title = {Predictors of Medication Self-Management Skill in a Low-Literacy Population},
  author = {Kripalani, S. and Henderson, L. E. and Chiu, E. Y. and Robertson, R. and Kolm, P. and Jacobson, T. A.},
