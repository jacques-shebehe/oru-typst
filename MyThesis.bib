@article{vanMunster-2020,
  title = {Improving Disease Knowledge of Primary Sclerosing Cholangitis Patients and Their Relatives with a 3-Dimensional Education Video},
  author = {{van Munster}, K. N. and {van Mil}, J. and Safer, R. and Nieuwkerk, P. T. and Ponsioen, C. Y.},
