Switzerland\\
Pflege. 2013 Oct;26(5):311-20. doi: 10.1024/1012-5302/a000315.}
}

@article{Lanini-2009,
  title = {Healthcare-Associated Infection in {{Italy}}: Annual Point-Prevalence Surveys, 2002-2004},
  author = {Lanini, S. and Jarvis, W. R. and Nicastri, E. and Privitera, G. and Gesu, G. and Marchetti, F. and Giuliani, L. and Piselli, P. and Puro, V. and Nisii, C. and Ippolito, G.},
  year = {2009},
