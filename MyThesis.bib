@article{Simmons-2017,
  title = {Health {{Literacy}}: {{Cancer Prevention Strategies}} for {{Early Adults}}},
  author = {Simmons, R. A. and Cosgrove, S. C. and Romney, M. C. and Plumb, J. D. and Brawer, R. O. and Gonzalez, E. T. and Fleisher, L. G. and Moore, B. S.},
