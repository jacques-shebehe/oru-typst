@article{Braes-2010,
  title = {Screening for Risk of Unplanned Readmission in Older Patients Admitted to Hospital: Predictive Accuracy of Three Instruments.},
  author = {Braes, Tom and Moons, Philip and Lipkens, Piet and Sterckx, Wendy and Sabbe, Marc and Flamaing, Johan and Boonen, Steven and Milisen, Koen},
  year = {2010},
  month = aug,
  journal = {Aging clinical and experimental research},
  volume = {22},
  number = {4},
  eprint = {21116125},
  eprinttype = {pubmed},
  pages = {345--51},
  doi = {10.1007/BF03324938},
  abstract = {BACKGROUND AND AIMS: Hospital readmission after discharge is an important clinical and health policy issue. We compared the predictive accuracy of the Identification of Seniors at Risk (ISAR), the Flemish version of the Triage Risk Screening Tool (TRST) and Variable Indicative of Placement risk (VIP) in assessing unplanned readmissions. METHODS: We included 213 patients ({$\geq$}65 years), hospitalized following admission to the emergency department. The ISAR, TRST and VIP were administered at admission. Unplanned readmissions were registered by telephone follow-up 14, 30 and 90 days post-discharge. RESULTS: Unplanned readmission rates were 6.8\%, 14.7\% and 23.5\% after 14, 30, and 90 days, respectively. The ISAR showed low to moderate sensitivity (54\%-69\%) and a high negative predictive value ({$\geq$}78\%) at all measurement points. Specificity and positive predictive value were low ({$\leq$}33\% and {$\leq$}24\%, respectively). The TRST had low to moderate sensitivity (42\%-67\%) and a high negative predictive value ({$\geq$}82\%). Specificity and positive predictive value were low ({$\leq$}45\% and {$\leq$}27\%, respectively). The VIP had very low sensitivity ({$\leq$}26\%) and high specificity ({$\geq$}80\%). Its negative predictive value was high ({$\geq$}79\%) and its positive predictive value was low ({$\leq$}22\%). CONCLUSIONS: Due to their moderate to low sensitivity, and low specificity and positive predictive value, none of the instruments was capable of accurately predicting unplanned readmission in older, hospitalized patients. Overall, reducing or increasing the original cut-off value by one point did not result in improved performance. Our findings suggest that these instruments lack the necessary sophistication to capture the complexity of (unplanned) readmissions.},
  keywords = {Aged,Aged 80 and over,Belgium,Emergency Service Hospital,Female,Hospitalization,Humans,Longitudinal Studies,Male,Patient Readmission,Patient Readmission: statistics & numerical data,Predictive Value of Tests,Prospective Studies,Risk Factors,Triage,Triage: statistics & numerical data}
}

