@article{Kajsa-2016,
  title = {Screening for Frailty among Older Emergency Department Visitors: {{Validation}} of the New {{FRESH-screening}} Instrument.},
  author = {Kajsa, E. and Katarina, W. and Sten, L. and Synneve, I. D.},
  year = {2016},
