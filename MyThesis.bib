  author = {Leak, Cardella and Goggins, Kathryn and Schildcrout, Jonathan S. and Theobald, Cecelia and Donato, Katharine M. and Bell, Susan P. and Schnelle, John and Kripalani, Sunil and Vanderbilt Inpatient Cohort, Study},
