@article{Visscher-2020,
  title = {Medication Self-Management Support for People with Diabetes and Low Health Literacy: {{A}} Needs Assessment},
  author = {Visscher, B. B. and Steunenberg, B. and Heerdink, E. R. and Rademakers, J.},
  year = {2020},
