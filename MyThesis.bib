@article{Rawle-2018,
  title = {The Prevalence and Determinants of Polypharmacy at Age 69: A {{British}} Birth Cohort Study},
  author = {Rawle, M. J. and Richards, M. and Davis, D. and Kuh, D.},
  year = {2018},
  month = may,
  journal = {BMC Geriatr},
  edition = {2018/05/18},
  volume = {18},
