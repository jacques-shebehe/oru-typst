  keywords = {*Neonatal Sepsis/epidemiology,Adult,Apgar Score,Cross-Sectional Studies,EPIDEMIOLOGY,Female,Humans,Infant Low Birth Weight,Infant Newborn,Logistic Models,Male,Mali/epidemiology,NEONATOLOGY,Paediatric infectious disease & immunisation,PERINATOLOGY,Pregnancy,Prevalence,Prospective Studies,Risk Factors,Young Adult},
  file = {/Users/m1pro14/Zotero/storage/P2UV8RMV/Traore-2025.pdf}
}

