@article{Lapostolle-2017,
  title = {Diabetes Education of Patients and Their Entourage: Out-of-Hospital National Study ({{EDUCATED}} 2)},
  author = {Lapostolle, F. and Hamdi, N. and Barghout, M. and Soulat, L. and Faucher, A. and Lambert, Y. and Peschanski, N. and {Ricard-Hibon}, A. and Chassery, C. and Roti, M. and Bounes, V. and Debaty, G. and Mokni, T. and Egmann, G. and Fort, P. A. and Boudenia, K. and Alayrac, L. and Safraou, M. and Galinski, M. and Adnet, F.},
