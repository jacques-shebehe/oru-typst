  author = {Fan, Jerome and Worster, Andrew and Fernandes, Christopher M. B.},
  year = {2006},
  journal = {The American journal of emergency medicine},
  volume = {24},
  number = {5},
  pages = {540--4},
  doi = {10.1016/j.ajem.2006.01.015},
  abstract = {PURPOSE: We sought to externally evaluate the predictive validity of the Triage Risk Screening Tool (TRST) for elderly patients in a Canadian setting. METHODS: A prospective, observational cohort study of a convenience sample of patients more than 64 years old was assessed using the TRST before discharge. The composite outcome of any emergency department (ED) revisit, hospital admission, or long-term care placement at 30 and 120 days was measured. Likelihood ratios (LRs) and 95\% confidence intervals (CIs) were calculated. MAIN FINDINGS: Of 218 patients screened, 120 patients were enrolled. At 30 and 120 days, the positive LRs were 1.4 (95\% CI, 0.9-2.0) and 1.4 (95\% CI, 1.0-1.9), respectively. The negative LRs were 0.7 (95\% CI, 0.4-1.3) and 0.7 (95\% CI, 0.4-1.0), respectively. CONCLUSION: The TRST cannot be used as a single diagnostic test to predict whether Canadian ED elders will have an ED revisit, hospital admission, or long-term care placement at 30 or 120 days.},
  langid = {english},
  keywords = {Aged,Aged 80 and over,Cohort Studies,Emergency Nursing,Emergency Nursing: instrumentation,Female,Geriatrics,Geriatrics: instrumentation,Humans,Male,Mass Screening,Mass Screening: instrumentation,Ontario,Predictive Value of Tests,Prospective Studies,Reproducibility of Results,Risk Assessment,Risk Assessment: methods,Triage,Triage: methods},
