@article{Roe-2017,
  title = {The Impact of Frailty on Healthcare Utilisation in {{Ireland}}: Evidence from the {{Irish}} Longitudinal Study on Ageing},
  author = {Roe, L. and Normand, C. and Wren, M. A. and Browne, J. and O'Halloran, A. M.},
