@article{Carey-2008,
  title = {What Factors Predict Potentially Inappropriate Primary Care Prescribing in Older People? {{Analysis}} of {{UK}} Primary Care Patient Record Database},
  author = {Carey, I. M. and De Wilde, S. and Harris, T. and Victor, C. and Richards, N. and Hilton, S. R. and Cook, D. G.},
