@article{Karin-2022,
  title = {Body Mass Index in Adolescence, Risk of Type 2 Diabetes and Associated Complications: {{A}} Nationwide Cohort Study of Men},
  shorttitle = {Body Mass Index in Adolescence, Risk of Type 2 Diabetes and Associated Complications},
  author = {Karin, Andr{\'e}asson and Jon, Edqvist and Martin, Adiels and Lena, Bj{\"o}rck and Martin, Lindgren and Naveed, Sattar and Marcus, Lind and Maria, {\AA}berg and Annika, Rosengren},
  year = {2022},
  month = mar,
  journal = {EClinicalMedicine},
  volume = {46},
  pages = {101356},
  issn = {2589-5370},
  doi = {10.1016/j.eclinm.2022.101356},
  urldate = {2023-10-31},
  abstract = {Background Obesity is a predominant factor in development of type 2 diabetes but to which extent adolescent obesity influences adult diabetes is unclear. We investigated the association between body mass index (BMI) in young men and subsequent type 2 diabetes and how, in diagnosed diabetes, adolescent BMI relates to glycemic control and diabetes complications. Methods Baseline data from the Swedish Conscript Register for men drafted 1968--2005 was combined with data from the National Diabetes and Patient registries. Diabetes risk was estimated through Cox regression and Kaplan-Meier survival estimates. Relationships between BMI, glycemic control and diabetes complications were assessed through multiple linear and logistic regression. Findings Among~1,647,826 men, 63,957 (3{$\cdot$}88\%) developed type 2 diabetes over a median follow-up of 29.0 years (IQR[21.0--37.0]). The risk of diabetes within 40 years after conscription was nearly 40\% in individuals with adolescent BMI~{$\geq$}35~kg/m2. Compared to BMI 18{$\cdot$}5--{$<$}20~kg/m2 (reference), diabetes risk increased in a linear fashion from HR 1{$\cdot$}18(95\%CI 1{$\cdot$}15--1{$\cdot$}21) for BMI 20--{$<$}22{$\cdot$}5~kg/m2 to HR 15{$\cdot$}93(95\%CI 14{$\cdot$}88--17{$\cdot$}05) for BMI {$\geq$}35~kg/m2, and a difference in age at onset of 11{$\cdot$}4 years was seen. Among men who developed diabetes, higher adolescent BMI was associated with higher HbA1c levels and albuminuria rates. Interpretation Rising adolescent BMI was associated with increased risk of type 2 diabetes diagnosed at a younger age, with poorer metabolic control, and a greater prevalence of albuminuria, all suggestive of worse prognosis.},
  pmcid = {PMC8938860},
  pmid = {35330801},
