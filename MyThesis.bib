@article{Kopanz-2022,
  title = {Digital Algorithm-Guided Insulin Therapy in Home Healthcare for Elderly Persons with Type 2 Diabetes: {{A}} Proof-of-Concept Study},
  shorttitle = {Digital Algorithm-Guided Insulin Therapy in Home Healthcare for Elderly Persons with Type 2 Diabetes},
  author = {Kopanz, Julia and Mader, Julia K. and Donsa, Klaus and Libiseller, Angela and Aberer, Felix and Pandis, Marlene and {Reinisch-Gratzer}, Johanna and Ambrosch, Gisela C. and Lackner, Bettina and Truskaller, Thomas and Sinner, Frank Michael and Pieber, Thomas R. and Lichtenegger, Katharina M.},
