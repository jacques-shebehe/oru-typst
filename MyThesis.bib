@article{Austin-2001,
  title = {Parents' Perceptions of Information on Immunisations},
  author = {Austin, H.},
  year = 2001,
  journal = {J Child Health Care},
  edition = {2002/02/22},
  volume = {5},
  number = {2},
  pages = {54--9},
  issn = {1367-4935 (Print)1367-4935},
  doi = {10.1177/136749350100500202},
  abstract = {This study sought to understand parents' experiences of deciding to have their child immunised. A qualitative method was used, involving semi-structured interviews with a purposive sample of 13 parents. Parents identified feelings associated with risk, including those of worry, fear, and responsibility. Influences affecting their decision-making, and ways in which they received information on immunisation, were also identified. Parents made suggestions for changes relating to information giving, discussion time, and the acknowledgement by professionals of parents' worry. Following this study, discussion within the PHCT has resulted in a practice protocol and immunisation template.},
  langid = {english},
  keywords = {*Communication,*Decision Making,Female,Humans,Immunization/*psychology,Interviews as Topic,Male,Parents/*psychology,Patient Care Team,Primary Health Care,Professional-Family Relations,Risk},
  note = {Austin, H\\
