  pmid = {39342103},
  keywords = {*Neonatal Sepsis/diagnosis/epidemiology,Adult,Cross-Sectional Studies,Duration of rupture of membrane,Early onset neonatal sepsis,Ethiopia/epidemiology,Female,Fetal Membranes Premature Rupture,Humans,Incidence,Infant Newborn,Male,Pregnancy,Premature rupture of membrane,Risk Factors,Vaginal examination},
