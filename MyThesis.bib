@article{Segelman-2014,
  title = {Hospitalizations in the {{Program}} of {{All-Inclusive Care}} for the {{Elderly}}.},
  author = {Segelman, Micah and Szydlowski, Jill and Kinosian, Bruce and McNabney, Matthew and Raziano, Donna B and Eng, Catherine and {van Reenen}, Christine and {Temkin-Greener}, Helena},
