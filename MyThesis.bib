@article{Scheller-2015,
  title = {Quadrivalent {{HPV}} Vaccination and Risk of Multiple Sclerosis and Other Demyelinating Diseases of the Central Nervous System},
  author = {Scheller, N. M. and Svanstrom, H. and Pasternak, B. and {Arnheim-Dahlstrom}, L. and Sundstrom, K. and Fink, K. and Hviid, A.},
  year = {2015},
