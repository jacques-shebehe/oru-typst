  keywords = {*Communication Barriers,*Physician-Patient Relations,Adult,Aged,Attitude of Health Personnel,barrier,Connecticut,conversation,facilitator,Female,goal,Humans,Male,Middle Aged,Neoplasms/*psychology/*therapy,New York,Oncologists/*psychology,palliative,Patient Care Planning/*organization & administration,Qualitative Research,Terminal Care/*psychology},
