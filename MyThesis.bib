@article{Rawshani-2018a,
  title = {Excess Mortality and Cardiovascular Disease in Young Adults with Type 1 Diabetes in Relation to Age at Onset: A Nationwide, Register-Based Cohort Study},
  shorttitle = {Excess Mortality and Cardiovascular Disease in Young Adults with Type 1 Diabetes in Relation to Age at Onset},
  author = {Rawshani, Araz and Sattar, Naveed and Franz{\'e}n, Stefan and Rawshani, Aidin and Hattersley, Andrew T. and Svensson, Ann-Marie and Eliasson, Bj{\"o}rn and Gudbj{\"o}rnsdottir, Soffia},
  year = {2018},
  month = aug,
  journal = {Lancet (London, England)},
  volume = {392},
  number = {10146},
  pages = {477--486},
  issn = {1474-547X},
  doi = {10.1016/S0140-6736(18)31506-X},
  abstract = {BACKGROUND: People with type 1 diabetes are at elevated risk of mortality and cardiovascular disease, yet current guidelines do not consider age of onset as an important risk stratifier. We aimed to examine how age at diagnosis of type 1 diabetes relates to excess mortality and cardiovascular risk. METHODS: We did a nationwide, register-based cohort study of individuals with type 1 diabetes in the Swedish National Diabetes Register and matched controls from the general population. We included patients with at least one registration between Jan 1, 1998, and Dec 31, 2012. Using Cox regression, and with adjustment for diabetes duration, we estimated the excess risk of all-cause mortality, cardiovascular mortality, non-cardiovascular mortality, acute myocardial infarction, stroke, cardiovascular disease (a composite of acute myocardial infarction and stroke), coronary heart disease, heart failure, and atrial fibrillation. Individuals with type 1 diabetes were categorised into five groups, according to age at diagnosis: 0-10 years, 11-15 years, 16-20 years, 21-25 years, and 26-30 years. FINDINGS: 27\hphantom{,}195 individuals with type 1 diabetes and 135\hphantom{,}178 matched controls were selected for this study. 959 individuals with type 1 diabetes and 1501 controls died during follow-up (median follow-up was 10 years). Patients who developed type 1 diabetes at 0-10 years of age had hazard ratios of 4{$\cdot$}11 (95\% CI 3{$\cdot$}24-5{$\cdot$}22) for all-cause mortality, 7{$\cdot$}38 (3{$\cdot$}65-14{$\cdot$}94) for cardiovascular mortality, 3{$\cdot$}96 (3{$\cdot$}06-5{$\cdot$}11) for non-cardiovascular mortality, 11{$\cdot$}44 (7{$\cdot$}95-16{$\cdot$}44) for cardiovascular disease, 30{$\cdot$}50 (19{$\cdot$}98-46{$\cdot$}57) for coronary heart disease, 30{$\cdot$}95 (17{$\cdot$}59-54{$\cdot$}45) for acute myocardial infarction, 6{$\cdot$}45 (4{$\cdot$}04-10{$\cdot$}31) for stroke, 12{$\cdot$}90 (7{$\cdot$}39-22{$\cdot$}51) for heart failure, and 1{$\cdot$}17 (0{$\cdot$}62-2{$\cdot$}20) for atrial fibrillation. Corresponding hazard ratios for individuals who developed type 1 diabetes aged 26-30 years were 2{$\cdot$}83 (95\% CI 2{$\cdot$}38-3{$\cdot$}37) for all-cause mortality, 3{$\cdot$}64 (2{$\cdot$}34-5{$\cdot$}66) for cardiovascular mortality, 2{$\cdot$}78 (2{$\cdot$}29-3{$\cdot$}38) for non-cardiovascular mortality, 3{$\cdot$}85 (3{$\cdot$}05-4{$\cdot$}87) for cardiovascular disease, 6{$\cdot$}08 (4{$\cdot$}71-7{$\cdot$}84) for coronary heart disease, 5{$\cdot$}77 (4{$\cdot$}08-8{$\cdot$}16) for acute myocardial infarction, 3{$\cdot$}22 (2{$\cdot$}35-4{$\cdot$}42) for stroke, 5{$\cdot$}07 (3{$\cdot$}55-7{$\cdot$}22) for heart failure, and 1{$\cdot$}18 (0{$\cdot$}79-1{$\cdot$}77) for atrial fibrillation; hence the excess risk differed by up to five times across the diagnosis age groups. The highest overall incidence rate, noted for all-cause mortality, was 1{$\cdot$}9 (95\% CI 1{$\cdot$}71-2{$\cdot$}11) per 100\hphantom{,}000 person-years for people with type 1 diabetes. Development of type 1 diabetes before 10 years of age resulted in a loss of 17{$\cdot$}7 life-years (95\% CI 14{$\cdot$}5-20{$\cdot$}4) for women and 14{$\cdot$}2 life-years (12{$\cdot$}1-18{$\cdot$}2) for men. INTERPRETATION: Age at onset of type 1 diabetes is an important determinant of survival, as well as all cardiovascular outcomes, with highest excess risk in women. Greater focus on cardioprotection might be warranted in people with early-onset type 1 diabetes. FUNDING: Swedish Heart and Lung Foundation.},
  langid = {english},
  pmcid = {PMC6828554},
  pmid = {30129464},
  keywords = {Adolescent,Adult,Age Factors,Age of Onset,Cardiovascular Diseases,Child,Child Preschool,Cohort Studies,Diabetes Complications,Diabetes Mellitus Type 1,Female,Humans,Infant,Infant Newborn,Male,Proportional Hazards Models,Registries,Sweden,Young Adult},
  file = {/Users/m1pro14/Zotero/storage/7PBKGRA8/Rawshani-2018a 2.pdf;/Users/m1pro14/Zotero/storage/YA5GSSTD/Rawshani-2018a 3.pdf}
}

@article{Reed-,
