  author = {Jia, Jianping and Zhao, Tan and Liu, Zhaojun and Liang, Yumei and Li, Fangyu and Li, Yan and Liu, Wenying and Li, Fang and Shi, Shengliang and Zhou, Chunkui and Yang, Heyun and Liao, Zhengluan and Li, Yang and Zhao, Huiying and Zhang, Jintao and Zhang, Kunnan and Kan, Minchen and Yang, Shanshan and Li, Hao and Liu, Zhongling and Ma, Rong and Lv, Jihui and Wang, Yue and Yan, Xin and Liang, Furu and Yuan, Xiaoling and Zhang, Jinbiao and Gauthier, Serge and Cummings, Jeffrey},
