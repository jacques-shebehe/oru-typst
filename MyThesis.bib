@article{Popescu-2020,
  title = {Neonatal Sepsis in Low-Income Countries: Epidemiology, Diagnosis and Prevention.},
  author = {Popescu, Constantin Radu and Cavanagh, Miranda M. M. and Tembo, Bentry and Chiume, Msandeni and Lufesi, Norman and Goldfarb, David M. and Kissoon, Niranjan and Lavoie, Pascal M.},
  year = {2020},
  month = may,
  journal = {Expert review of anti-infective therapy},
  volume = {18},
  number = {5},
  pages = {443--452},
  address = {England},
  issn = {1744-8336 1478-7210},
  doi = {10.1080/14787210.2020.1732818},
  abstract = {Introduction: Sepsis accounts for up to one-third of neonatal deaths in the world each year. The World Health Organization acknowledges neonatal sepsis as a major  global health concern, and that the highest burden occurs in low- and  middle-income countries (LMICs). Despite major research and clinical progress in  this area, we still lack accurate diagnostic tools for neonatal sepsis,  complicating the management of this condition.Areas covered: The purpose here is  to review the latest data on the incidence, diagnosis, prevention, and management  of neonatal sepsis in LMIC. We discuss the limitations of current diagnostic  tests - including their lack of availability - and how this may influence global  estimates of cases. We review the benefits of antenatal, intrapartum, and  post-natal preventive measures. We briefly discuss the management, highlighting  the emergence of antimicrobial resistance. Finally, we expose some high priority  areas.Expert opinion: Neonatal sepsis is a challenging condition requiring a  multifaceted approach to address the major diagnostic issues, but also the  underlying socio-economic causes that nourish epidemic cases in LMIC. Focusing on  antibiotics as a main pillar of intervention is likely to engender antimicrobial  resistance, eventually hindering the appreciable gains LMICs have achieved in  neonatal health outcomes.},
  langid = {english},
  pmid = {32070161},
  keywords = {*Developing Countries,Anti-Bacterial Agents/administration & dosage,Drug Resistance Bacterial,Female,Humans,Incidence,Infant Newborn,low- and middle-income countries,Neonatal sepsis,neonatal sepsis diagnosis and antimicrobial resistance,neonatal sepsis epidemiology,Neonatal Sepsis/diagnosis/*epidemiology/prevention & control,Pregnancy,Prenatal Care/methods,Socioeconomic Factors},
