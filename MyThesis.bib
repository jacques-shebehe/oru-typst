@article{Wastesson-2018,
  title = {The Composition of Polypharmacy: {{A}} Register-Based Study of {{Swedes}} Aged 75 Years and Older},
  author = {Wastesson, J. W. and Cedazo Minguez, A. and Fastbom, J. and Maioli, S. and Johnell, K.},
  year = {2018},
  journal = {PLoS One},
