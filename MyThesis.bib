@article{Rosland-2010,
  title = {Family Influences on Self-Management among Functionally Independent Adults with Diabetes or Heart Failure: Do Family Members Hinder as Much as They Help?},
  author = {Rosland, A. M. and Heisler, M. and Choi, H. J. and Silveira, M. J. and Piette, J. D.},
