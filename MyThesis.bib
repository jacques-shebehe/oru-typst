  author = {Tsuzuki, Shinya and Fujitsuka, Niina and Horiuchi, Keisuke and Ijichi, Shinpei and Gu, Yoshiaki and Fujitomo, Yumiko and Takahashi, Rie and Ohmagari, Norio},
  year = {2020},
