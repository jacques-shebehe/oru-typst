@article{Ludvigsson-2006,
  title = {Celiac Disease and Risk of Subsequent Type 1 Diabetes: A General Population Cohort Study of Children and Adolescents},
  author = {Ludvigsson, J. F. and Ludvigsson, J. and Ekbom, A. and Montgomery, S. M.},
