@article{Tsai-2018,
  title = {Impact of a {{Problem-Based Learning}} ({{PBL}}) {{Health Literacy Program}} on {{Immigrant Women}}'s {{Health Literacy}}, {{Health Empowerment}}, {{Navigation Efficacy}}, and {{Health Care Utilization}}},
  author = {Tsai, T. I. and Lee, S. D. and Yu, W. R.},
