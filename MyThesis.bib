@article{Glanz-2018,
  title = {Association {{Between Estimated Cumulative Vaccine Antigen Exposure Through}} the {{First}} 23 {{Months}} of {{Life}} and {{Non-Vaccine-Targeted Infections From}} 24 {{Through}} 47 {{Months}} of {{Age}}},
  author = {Glanz, J. M. and Newcomer, S. R. and Daley, M. F. and DeStefano, F. and Groom, H. C. and Jackson, M. L. and Lewin, B. J. and McCarthy, N. L. and McClure, D. L. and Narwaney, K. J. and Nordin, J. D. and Zerbo, O.},
  year = {2018},
