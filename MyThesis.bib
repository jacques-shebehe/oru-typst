@article{Herrin-2015,
  title = {Community {{Factors}} and {{Hospital Readmission Rates}}},
  author = {Herrin, Jeph and IL, Health Research \& Educational Trust Chicago and 22902, Yale University School of Medicine Division of Cardiology New Haven CT VA and St. Andre, Justin and Navigant Consulting, Inc. Chicago IL and Kenward, Kevin and IL, Health Research \& Educational Trust Chicago and Joshi, Maulik S. and IL, Health Research \& Educational Trust Chicago and Audet, Anne-Marie J. and NY, The Commonwealth Fund department of Delivery System Reform {and} Breakthrough Opportunities New York and Hines, Stephen C. and IL, Health Research \& Educational Trust Chicago},
