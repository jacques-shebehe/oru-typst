  author = {Doubova, S. V. and Infante, C. and {Villagrana-Guti{\'e}rrez}, G. L. and {Mart{\'i}nez-Vega}, I. P. and {P{\'e}rez-Cuevas}, R.},
  year = {2019},
  month = aug,
  journal = {Psychol Health Med},
  edition = {2019/02/02},
  volume = {24},
