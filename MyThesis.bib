@article{Gudayu-2019,
  title = {The Role of the Season at Admission in Neonatal Sepsis: A Retrospective Chart Review of a 1-Year Data at {{University}} of {{Gondar}} Comprehensive Specialized  Hospital.},
  author = {Gudayu, Temesgen Worku and Zeleke, Ejigu Gebeye and Lakew, Ayenew Molla},
  year = {2019},
