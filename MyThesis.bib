@article{Willeboordse-2016,
  title = {Information on Actual Medication Use and Drug-Related Problems in Older Patients: Questionnaire or Interview?},
  author = {Willeboordse, F. and Grundeken, L. H. and {van den Eijkel}, L. P. and Schellevis, F. G. and Elders, P. J. and Hugtenburg, J. G.},
