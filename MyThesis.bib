@article{Huang-2019,
  title = {The Impact of Health Literacy on Knowledge, {{Attitude}} and Decision towards Hospice Care among Community-Dwelling Seniors},
  author = {Huang, H. Y. and Kuo, K. M. and Lu, I. C. and Wu, H. and Lin, C. W. and Hsieh, M. T. and Lin, Y. C. and Huang, R. Y. and Liu, I. T. and Huang, C. H.},
