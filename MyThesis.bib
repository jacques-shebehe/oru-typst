@article{Dely-2012,
  title = {[{{Preventable}} Readmissions of "Community-Acquired Pneumonia": {{Usefulness}} and Reliability of an Indicator of the Quality of Care of Patients' Care Pathways].},
  author = {Dely, Claire and Sellier, Pierre and Dozol, Adrien and Segouin, Christophe and Moret, Leila and Lombrail, Pierre},
  year = {2012},
  month = jan,
  journal = {Presse m{\'e}dicale (Paris, France : 1983)},
  volume = {41},
  number = {1},
  eprint = {21802247},
  eprinttype = {pubmed},
  pages = {e1-9},
  doi = {10.1016/j.lpm.2011.06.007},
  abstract = {UNLABELLED: Unlike Anglo-Saxon countries, the rate of preventable readmissions is not a national quality indicator in France. In the context of the European research project Performance Assessment Tool for quality improvement in Hospitals (PATH), this pilot study provides a tool for health professionals to more easily identify preventable readmissions in order to analyze their causes. The objective of this work was to show the reliability of this tool. METHODS: We compared the rate of preventable readmissions occurring within 30 days, for a given disease, collected by analyzing medical reports (the gold standard rate of this study) to the rates of preventable readmissions occurring within 30 days, measured from algorithms using data from the French Programme of Medicalization of Information Systems (PMSI). The data were derived from PMSI data of the Lariboisiere-Fernand-Widal Hospital, a university hospital in Paris. RESULTS: The community-acquired pneumonia gold standard rate of preventable readmissions occurring within 30 days was 6.3 \%. Five algorithmic tools were used and measured rates between 2.9 \% and 7.9 \%. The positive likelihood ratios of these tools range from 8 to 308. CONCLUSION: This work presents a tool aimed at the health professionals that takes into account the diversity and complexity of the causes of avoidable readmissions, resulting from the quality of medical practice, the variability of the organization of hospital pathways and the links with ambulatory care.},
  keywords = {Aged,Algorithms,Community-Acquired Infections,Community-Acquired Infections: epidemiology,Community-Acquired Infections: prevention & contro,Community-Acquired Infections: therapy,Community-Acquired Infections: transmission,Continuity of Patient Care,Continuity of Patient Care: standards,Continuity of Patient Care: statistics & numerical,Female,Forecasting,France,Hospitals University,Humans,Male,Paris,Patient Readmission,Patient Readmission: statistics & numerical data,Pilot Projects,Pneumonia,Pneumonia: epidemiology,Pneumonia: prevention & control,Pneumonia: therapy,Predictive Value of Tests,Quality Indicators Health Care,Quality Indicators Health Care: standards,Quality Indicators Health Care: statistics & nume,Quality of Health Care,Quality of Health Care: statistics & numerical dat,Reproducibility of Results}
}

@article{Dendale-2012,
  title = {Effect of a Telemonitoring-Facilitated Collaboration between General Practitioner and Heart Failure Clinic on Mortality and Rehospitalization Rates in Severe Heart Failure: The {{TEMA-HF}} 1 ({{TElemonitoring}} in the {{MAnagement}} of {{Heart Failure}}) Study.},
  author = {Dendale, Paul and De Keulenaer, Gilles and Troisfontaines, Pierre and Weytjens, Caroline and Mullens, Wilfried and Elegeert, Ivan and Ector, Bavo and Houbrechts, Marita and Willekens, Koen and Hansen, Dominique},
  year = {2012},
  month = mar,
  journal = {European journal of heart failure},
  volume = {14},
  number = {3},
  eprint = {22045925},
  eprinttype = {pubmed},
  pages = {333--40},
  doi = {10.1093/eurjhf/hfr144},
  abstract = {AIMS: Chronic heart failure (CHF) patients are frequently rehospitalized within 6 months after an episode of fluid retention. Rehospitalizations are preventable, but this requires an extensive organization of the healthcare system. In this study, we tested whether intensive follow-up of patients through a telemonitoring-facilitated collaboration between general practitioners (GPs) and a heart failure clinic could reduce mortality and rehospitalization rate. METHODS AND RESULTS: One hunderd and sixty CHF patients [mean age 76 {\textpm} 10 years, 104 males, mean left ventricular ejection fraction (LVEF) 35 {\textpm} 15\%] were block randomized by sealed envelopes and assigned to 6 months of intense follow-up facilitated by telemonitoring (TM) or usual care (UC). The TM group measured body weight, blood pressure, and heart rate on a daily basis with electronic devices that transferred the data automatically to an online database. Email alerts were sent to the GP and heart failure clinic to intervene when pre-defined limits were exceeded. All-cause mortality was significantly lower in the TM group as compared with the UC group (5\% vs. 17.5\%, P = 0.01). The total number of follow-up days lost to hospitalization, dialysis, or death was significantly lower in the TM group as compared with the UC group (13 vs. 30 days, P = 0.02). The number of hospitalizations for heart failure per patient showed a trend (0.24 vs. 0.42 hospitalizations/patient, P = 0.06) in favour of TM. CONCLUSION: Telemonitoring-facilitated collaboration between GPs and a heart failure clinic reduces mortality and number of days lost to hospitalization, death, or dialysis in CHF patients. These findings need confirmation in a large trial.},
  keywords = {Aged,Analysis of Variance,Antihypertensive Agents,Antihypertensive Agents: therapeutic use,Cardiac Care Facilities,Cardiac Care Facilities: statistics & numerical da,Cooperative Behavior,Female,General Practitioners,General Practitioners: statistics & numerical data,Heart Failure,Heart Failure: drug therapy,Heart Failure: mortality,Hospitalization,Hospitalization: statistics & numerical data,Humans,Male,Risk Factors,Severity of Illness Index,Stroke Volume,Telemedicine,Telemedicine: instrumentation,Telemedicine: methods,Treatment Failure,Ventricular Function Left}
}

