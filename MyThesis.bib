@article{Kobayashi-2016a,
  title = {Aging and {{Functional Health Literacy}}: {{A Systematic Review}} and {{Meta-Analysis}}},
  author = {Kobayashi, L. C. and Wardle, J. and Wolf, M. S. and {von Wagner}, C.},
