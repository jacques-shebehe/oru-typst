@article{Howard-2005,
  title = {The Impact of Low Health Literacy on the Medical Costs of {{Medicare}} Managed Care Enrollees},
  author = {Howard, D. H. and Gazmararian, J. and Parker, R. M.},
  year = {2005},
  month = apr,
