@article{NevesE-2021,
  title = {Oral Health Literacy, Sociodemographic, Family, and Clinical Predictors of Dental Visits among {{Brazilian}} Early Adolescents},
  author = {Neves {\'E}, T. B. and Lima, L. C. M. and Dutra, L. D. C. and Gomes, M. C. and Siqueira, Mbld and Paiva, S. M. and Ferreira, F. M. and {Granville-Garcia}, A. F.},
  year = {2021},
  month = mar,
  journal = {Int J Paediatr Dent},
