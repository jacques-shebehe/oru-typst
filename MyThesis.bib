@article{Bergh-2015,
  title = {Stress Resilience and Physical Fitness in Adolescence and Risk of Coronary Heart Disease in Middle Age},
  author = {Bergh, C. and Udumyan, R. and Fall, K. and Almroth, H. and Montgomery, S.},
