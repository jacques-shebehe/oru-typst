Int J Epidemiol. 2014 Feb;43(1):264-9. doi: 10.1093/ije/dyt221. Epub 2013 Dec 23.}
}

@article{Gunn-2020,
  title = {Health {{Literacy}}, {{Language}}, and {{Cancer-Related Needs}} in the {{First}} 6 {{Months After}} a {{Breast Cancer Diagnosis}}},
  author = {Gunn, C. M. and {Paasche-Orlow}, M. K. and Bak, S. and Wang, N. and Pamphile, J. and Nelson, K. and Morton, S. and Battaglia, T. A.},
  year = {2020},
  month = aug,
  journal = {JCO Oncol Pract},
  edition = {2020/03/29},
  volume = {16},
