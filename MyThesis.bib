  title = {Medication {{Discrepancies}} in {{Older Veterans Receiving Home Healthcare}}},
  author = {Mixon, A. S. and Poppendeck, H. and Kripalani, S. and Speroff, T. and Schnelle, J. and Dittus, R. and Roumie, C.},
