@article{Gallagher-Thompson-2015,
  title = {Effectiveness of a Fotonovela for Reducing Depression and Stress in {{Latino}} Dementia Family Caregivers},
  author = {{Gallagher-Thompson}, D. and Tzuang, M. and Hinton, L. and Alvarez, P. and Rengifo, J. and Valverde, I. and Chen, N. and Emrani, T. and Thompson, L. W.},
  year = {2015},
  month = apr,
  journal = {Alzheimer Dis Assoc Disord},
  edition = {2015/01/16},
  volume = {29},
  number = {2},
