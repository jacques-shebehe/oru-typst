@article{Heath-2016,
  title = {Status of Vaccine Research and Development of Vaccines for {{GBS}}.},
  author = {Heath, Paul T.},
  year = {2016},
  month = jun,
  journal = {Vaccine},
  volume = {34},
  number = {26},
  pages = {2876--2879},
