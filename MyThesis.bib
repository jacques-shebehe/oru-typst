}

@article{McNaughton-2014,
  title = {Association of Health Literacy with Elevated Blood Pressure: A Cohort Study of Hospitalized Patients},
  author = {McNaughton, C. D. and Kripalani, S. and Cawthon, C. and Mion, L. C. and Wallston, K. A. and Roumie, C. L.},
  year = {2014},
  month = apr,
  journal = {Med Care},
  volume = {52},
  number = {4},
  pages = {346--53},
  issn = {1537-1948 (Electronic) 0025-7079 (Print) 0025-7079 (Linking)},
  doi = {10.1097/MLR.0000000000000101},
  abstract = {BACKGROUND: The clinical consequences of low health literacy are not fully understood. OBJECTIVES: We evaluated the relationship between low health literacy and elevated blood pressure (BP) at hospital presentation. RESEARCH DESIGN AND SUBJECTS: We conducted a cross-sectional evaluation of adult patients hospitalized at a university hospital between November 1, 2010 and April 30, 2012. MEASURES: Health literacy was assessed using the Brief Health Literacy Screen (BHLS). Low health literacy was defined as a BHLS score {$\leq$}9. BP was assessed using clinical measurements. The outcome was elevated BP ({$\geq$}140/90 mm Hg; {$\geq$}130/80 mm Hg with diabetes or renal disease) or extremely elevated BP ({$>$}160/100 mm Hg) at hospital presentation. Multivariate logistic regression adjusted for age, sex, race, insurance, comorbidities, and antihypertensive medications; preplanned restricted analysis among patients with diagnosed hypertension was performed. RESULTS: Of 46,263 hospitalizations, 23\% had low health literacy, which occurred more often among patients who were older (61 vs. 54 y), less educated (28.4\% vs. 11.2\% had not completed high school), and more often admitted through the emergency department (54.3\% vs. 48.1\%) than those with BHLS{$>$}9. Elevated BP was more frequent among those with low health literacy [40.0\% vs. 35.5\%; adjusted odds ratio (aOR) 1.06; 95\% confidence interval (CI), 1.01-1.12]. Low health literacy was associated with extremely elevated BP (aOR 1.08; 95\% CI, 1.01-1.16) and elevated BP among those without diagnosed hypertension (aOR 1.09; 95\% CI, 1.02-1.16). CONCLUSIONS: More than {$\frac{1}{3}$} of patients had elevated BP at hospital presentation. Low health literacy was independently associated with elevated BP, particularly among patients without diagnosed hypertension.},
  langid = {english},
  keywords = {Adult,Age Factors,Aged,Blood Pressure,Cross-Sectional Studies,Educational Status,Female,Health Literacy/statistics & numerical data,Hospitalization/statistics & numerical data,Humans,Hypertension/ epidemiology,Male,Middle Aged},
