@article{Kwok-1999,
  title = {Hospital Readmission among Older Medical Patients in {{Hong Kong}}},
  author = {Kwok, T. and Lau, E. and Woo, J. and Luk, J. K. and Wong, E. and Sham, A. and Lee, S. H.},
