@article{Mayberry-2014,
  title = {Family Members' Obstructive Behaviors Appear to Be More Harmful among Adults with Type 2 Diabetes and Limited Health Literacy},
  author = {Mayberry, L. S. and Rothman, R. L. and Osborn, C. Y.},
  year = {2014},
  journal = {J Health Commun},
