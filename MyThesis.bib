  keywords = {Aged,Cohort Studies,Female,Health Literacy/*statistics & numerical data,Health Services Accessibility/*statistics & numerical data,Humans,Male,Middle Aged,Self Report,Socioeconomic Factors,Time-to-Treatment/statistics & numerical data},
