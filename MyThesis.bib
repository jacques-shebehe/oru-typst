@article{Molstad-2017,
  title = {Lessons Learnt during 20 Years of the {{Swedish}} Strategic Programme against Antibiotic Resistance},
  author = {M{\"o}lstad, Sigvard and L{\"o}fmark, Sonja and Carlin, Karin and Erntell, Mats and Aspevall, Olov and Blad, Lars and Hanberger, H{\aa}kan and Hedin, Katarina and Hellman, Jenny and Norman, Christer and Skoog, Gunilla and {St{\aa}lsby-Lundborg}, Cecilia and Tegmark Wisell, Karin and {\AA}hr{\'e}n, Christina and Cars, Otto},
