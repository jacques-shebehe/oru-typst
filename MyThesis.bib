@article{Eveillard-2001,
  title = {Evaluation of the Contribution of Isolation Precautions in Prevention and Control of Multi-Resistant Bacteria in a Teaching Hospital},
  author = {Eveillard, M. and Eb, F. and Tramier, B. and Schmit, J. L. and Lescure, F. X. and Biendo, M. and Canarelli, B. and Daoudi, F. and Laurans, G. and Rousseau, F. and Thomas, D.},
  year = {2001},
