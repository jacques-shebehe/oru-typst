}

@article{Pittet-2004,
  title = {Hand Hygiene among Physicians: Performance, Beliefs, and Perceptions},
  author = {Pittet, D. and Simon, A. and Hugonnet, S. and {Pessoa-Silva}, C. L. and Sauvan, V. and Perneger, T. V.},
  year = {2004},
