@article{Mayo-Gamble-2018,
  title = {Examining the {{Association Between Health Literacy}} and {{Medication Adherence Among Older Adults}}},
  author = {{Mayo-Gamble}, T. L. and Mouton, C.},
  year = {2018},
  month = sep,
  journal = {Health Commun},
