@article{Li-2023,
  title = {Association of Body Indices and Risk of Mortality in Patients with Type 2 Diabetes},
  author = {Li, Chia-Ing and Liu, Chiu-Shong and Lin, Chih-Hsueh and Yang, Shing-Yu and Li, Tsai-Chung and Lin, Cheng-Chieh},
