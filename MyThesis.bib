@article{Leppin-2014,
  title = {Preventing 30-Day Hospital Readmissions: A Systematic Review and Meta-Analysis of Randomized Trials},
  author = {Leppin, A. L. and Gionfriddo, M. R. and Kessler, M. and Brito, J. P. and Mair, F. S. and Gallacher, K. and Wang, Z. and Erwin, P. J. and Sylvester, T. and Boehmer, K. and Ting, H. H. and Murad, M. H. and Shippee, N. D. and Montori, V. M.},
  year = {2014},
