@article{Emiral-2021,
  title = {Assessment of Knowledge of Metabolic Syndrome and Health Literacy Level among Adults in {{Western Turkey}}},
  author = {Emiral, G. O. and Tozun, M. and Atalay, B. I. and Goktas, S. and Dagtekin, G. and Aygar, H. and Arslantas, D. and Unsal, A. and Babaoglu, A. B. and Tirpan, K.},
  year = {2021},
