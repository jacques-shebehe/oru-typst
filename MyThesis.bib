Health Lit Res Pract. 2018 Dec 6;2(4):e221-e230. doi: 10.3928/24748307-20181025-01. eCollection 2018 Oct.},
  file = {/Users/m1pro14/Zotero/storage/T96LX7L2/Shrestha-2018.pdf}
}

@article{Siebert-2013,
  title = {{[The PRISCUS list in clinical routine. Practicability and comparison to international PIM lists]}},
  author = {Siebert, S. and Elkeles, B. and Hempel, G. and Kruse, J. and Smollich, M.},
